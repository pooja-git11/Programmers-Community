//reverse the digit of given input number
#include <iostream>
#include <vector>
using namespace std;

int main() {
    int num;
    vector <int>v1;
    cout<<"Enter a number";
    cin>>num;
     cout<<"\nReverse of " <<num<<" is:";
    while(num!=0)
    {
        v1.push_back(num%10);
        num/=10;
    }
    
    for(int i=0;i<v1.size();i++)
        cout<<v1[i];
        
    
    return 0;
}
