#include <iostream>
using namespace std;

int main() {

    int size,highest_element,m=0,var;
    cout<<"Enter size of array:";
    cin>>size;
    cout<<endl;
    cout<<"Enter the boundary element :";
    cin>>highest_element;
    int arr[size];
    cout<<"\nEnter sorted array element:";
    for(int i=0;i<size;i++)
    {
          cin>>var;
          if(var>highest_element)
             cout<<"enter valid range element";
          else
              arr[i]=var;
    }
    for(int i=0;i<=size;i++)
    {
        if(arr[i]!=m)
            break;
        m++;
    }
    cout<<"\nsmallest missing value  is :";
    cout<<m;
    
    
    return 0;
}
